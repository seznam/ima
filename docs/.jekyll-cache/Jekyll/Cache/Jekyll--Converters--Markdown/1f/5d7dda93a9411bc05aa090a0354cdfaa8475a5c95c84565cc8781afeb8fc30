  <thead>
    <tr>
      <th>Param</th>
      <th>Type</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>value</td>
      <td><code>\*</code></td>
      <td>The value to clone.</td>
    </tr>
  </tbody>
</table>

